<% flash.each do |type, msg| %>
  <div>
    <%= "#{type} : #{msg}" %>
  </div>
<% end %>

<p>In room <%= @room_id %></p>
<input id="room-id" hidden data-room-id="<%= @room_id %>"/>
<input id="session-id" hidden data-session-id="<%= session[:session_id] %>"/>

<%= link_to "Close room", session_destroy_path(@room_id), method: :delete %>
